.exp-player {
  @require "app/ui/stylus/normalize.styl";
  @require "app/ui/stylus/reset.styl";
  @require "app/ui/stylus/variables.styl";
  @require "app/ui/stylus/mixins.styl";
  overflow: hidden;
  padding-top: 56.25%;
  height: 0;
  position: relative;

  * {
    &,
    &:before,
    &:after {
      box-sizing: border-box;
    }
  }
}

 .exp-video {
   width: 100%;
   height: 100%;
   position: absolute;
   top: 0;
   left: 0;
   background: $black;
   background-image: image("ikon_dela.svg");
 }
