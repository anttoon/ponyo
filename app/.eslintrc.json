{
  "extends": "../.eslintrc.json",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
       "experimentalObjectRestSpread": true
     }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "Feature": false,
    "Scenario": false,
    "Given": false,
    "When": false,
    "Then": false,
    "And": false,
    "But": false,
    "I": false,
    "System": false,
    "Describe": false
  }
}
