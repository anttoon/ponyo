{
  "name": "nodestarterapp",
  "version": "0.0.2",
  "description": "Empty starter app for Expressen node apps",
  "main": "app.js",
  "homepage": "https://github.com/ExpressenAB/node-starterapp",
  "repository": {
    "type": "git",
    "url": "git@github.com:ExpressenAB/node-starterapp.git"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "config": {
    "exp-containership": {
      "salt": "https://localhost:8000",
      "environments": {
        "epitest": {
          "helios_deployment_group": "nodestarterapp-epitest"
        },
        "epistage": {
          "helios_deployment_group": "nodestarterapp-epistage"
        },
        "livedata": {
          "helios_deployment_group": "nodestarterapp-livedata"
        },
        "production": {
          "helios_deployment_group": "nodestarterapp-production"
        }
      }
    }
  },
  "scripts": {
    "init": "exp-containership init",
    "reset": "exp-containership reset",
    "test": "mocha && jshint . && jscs .",
    "build": "exp-containership build",
    "ctest": "exp-containership test",
    "start": "exp-containership run",
    "ci": "npm install && npm prune && npm ls && npm test",
    "prepush": "exp-ensure-unmodified && exp-ensure-master",
    "push": "exp-containership test && exp-containership push",
    "jobs": "exp-containerdeploy jobs -e",
    "status": "exp-containerdeploy status -e",
    "deploy": "exp-containerdeploy deploy -e",
    "undeploy": "exp-containerdeploy undeploy -e",
    "test-cov": "./node_modules/.bin/istanbul cover --report lcov ./node_modules/.bin/_mocha"
  },
  "author": "Expressen AB",
  "license": "proprietary",
  "private": true,
  "dependencies": {
    "compression": "^1.3.0",
    "dotenv": "^0.4.0",
    "exp-cachebuster": "^1.0.1",
    "exp-config": "^1.0.0",
    "express": "^4.11.0",
    "log": "^1.4.0",
    "morgan": "^1.5.1"
  },
  "devDependencies": {
    "browserify": "^10.1.0",
    "chai": "^1.10.0",
    "coffee-script": "^1.9.0",
    "exp-containership": "^0.1.2",
    "exp-deploy": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.5.2",
    "gulp-jscs": "^1.6.0",
    "gulp-jshint": "^1.10.0",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.4.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-stylint": "^1.1.2",
    "gulp-stylus": "^2.0.1",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "istanbul": "^0.3.15",
    "jscs": "^1.11.3",
    "jshint": "^2.6.0",
    "jshint-stylish": "^1.0.0",
    "mocha": "^2.1.0",
    "mocha-cakes": "^0.9.0",
    "supertest": "^0.15.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "files": [
    "app",
    "config",
    "lib",
    "public",
    "test",
    "app.js",
    "cluster.js",
    ".jshintrc",
    ".jshintignore",
    ".jscsrc"
  ]
}
