{
  "name": "nodestarterapp",
  "version": "0.0.2",
  "description": "Empty starter app for Expressen node apps",
  "main": "app.js",
  "homepage": "https://github.com/ExpressenAB/node-starterapp",
  "repository": {
    "type": "git",
    "url": "git@github.com:ExpressenAB/node-starterapp.git"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "config": {
    "exp-deploy": {
      "environments": {
        "epitest": {
          "servers": [
            "node-test-5.expressen.se"
          ]
        },
        "epistage": {
          "servers": [
            "node-test-6.expressen.se"
          ]
        },
        "livedata": {
          "waitForLoadbalancer": true,
          "servers": [
            "node-test-2.expressen.se",
            "node-test-3.expressen.se",
            "node-test-4.expressen.se"
          ]
        },
        "production": {
          "servers": [
            "your-prod-server-1",
            "your-prod-server-2"
          ]
        }
      }
    }
  },
  "scripts": {
    "test": "mocha && jshint . && jscs .",
    "revision": "which git && [[ -d .git ]] && git rev-parse HEAD > config/_revision || true",
    "ci": "npm install && npm prune && npm ls && npm test",
    "deploy-epitest": "exp-deploy epitest",
    "deploy-epistage": "exp-deploy epistage",
    "deploy-livedata": "exp-deploy livedata",
    "deploy-production": "exp-deploy production",
    "predeploy-production": "exp-ensure-tests && exp-ensure-unmodified && exp-ensure-master",
    "test-cov": "./node_modules/.bin/istanbul cover --report lcov ./node_modules/.bin/_mocha"
  },
  "author": "Expressen AB",
  "license": "proprietary",
  "private": true,
  "dependencies": {
    "compression": "^1.6.0",
    "dotenv": "^0.4.0",
    "exp-cachebuster": "^1.0.1",
    "exp-config": "^1.2.0",
    "express": "^4.13.3",
    "log": "^1.4.0",
    "morgan": "^1.6.1"
  },
  "devDependencies": {
    "browserify": "^10.2.6",
    "chai": "^1.10.0",
    "coffee-script": "^1.10.0",
    "exp-deploy": "0.0.6",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^1.0.0",
    "gulp-jscs": "^1.6.0",
    "gulp-jshint": "^1.11.2",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.4.3",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylint": "^1.1.10",
    "gulp-stylus": "^2.1.0",
    "gulp-uglify": "^1.4.1",
    "gulp-util": "^3.0.6",
    "istanbul": "^0.3.22",
    "jscs": "^2.3.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "^1.0.2",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "mocha-cakes": "^0.9.0",
    "supertest": "^0.15.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "files": [
    "app",
    "config",
    "lib",
    "public",
    "app.js",
    "cluster.js",
    ".nvmrc"
  ]
}
