{
  "name": "expstarterapp",
  "version": "0.0.2",
  "description": "Empty starter app for Expressen node apps",
  "main": "app.js",
  "homepage": "https://github.com/ExpressenAB/node-starterapp",
  "repository": {
    "type": "git",
    "url": "git@github.com:ExpressenAB/node-starterapp.git"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "mocha && jshint . && jscs .",
    "prepublish": "npm run-script revision && if which grunt ; then grunt dist ; else echo 'no grunt' ; fi",
    "revision": "which git && [[ -d .git ]] && git rev-parse HEAD > config/_revision || true",
    "ci": "npm install && npm prune && npm ls && npm test",
    "deploy-epitest": "./scripts/deploy.sh epitest",
    "deploy-epistage": "./scripts/deploy.sh epistage",
    "deploy-livedata": "./scripts/deploy.sh livedata",
    "deploy-production": "./scripts/deploy.sh production"
  },
  "author": "Expressen AB",
  "license": "proprietary",
  "private": true,
  "dependencies": {
    "compression": "^1.3.0",
    "dotenv": "^0.4.0",
    "exp-config": "^1.0.0",
    "express": "^4.11.0",
    "log": "^1.4.0",
    "morgan": "^1.5.1"
  },
  "devDependencies": {
    "browserify": "^10.1.0",
    "chai": "^1.10.0",
    "coffee-script": "^1.9.0",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.5.2",
    "gulp-jscs": "^1.6.0",
    "gulp-jshint": "^1.10.0",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^1.3.3",
    "gulp-scss-lint": "^0.1.12",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "jscs": "^1.11.3",
    "jshint": "^2.6.0",
    "jshint-stylish": "^1.0.0",
    "mocha": "^2.1.0",
    "mocha-cakes": "^0.9.0",
    "supertest": "^0.15.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "files": [
    "app",
    "config",
    "lib",
    "public",
    "app.js",
    "cluster.js",
    ".nvmrc"
  ]
}
