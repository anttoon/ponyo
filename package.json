{
  "name": "nodestarterapp",
  "version": "0.0.2",
  "description": "Empty starter app for Expressen node backend apps",
  "main": "app.js",
  "homepage": "https://github.com/ExpressenAB/node-starterapp",
  "repository": {
    "type": "git",
    "url": "git@github.com:ExpressenAB/node-starterapp.git"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "ci": "npm install && npm prune && npm ls && npm test",
    "deploy-production": "ohoy run deploy production",
    "dev": "npm run build && npm run watch",
    "predeploy-production": "exp-ensure-tests && exp-ensure-unmodified && exp-ensure-master",
    "prepublish": "npm run revision",
    "revision": "which git && [[ -d .git ]] && git rev-parse HEAD > config/_revision || true",
    "test": "mocha && eslint . --cached",
    "test-cov": "./node_modules/.bin/istanbul cover --report lcov ./node_modules/.bin/_mocha"
  },
  "author": "Expressen AB",
  "license": "proprietary",
  "private": true,
  "dependencies": {
    "compression": "^1.6.2",
    "exp-cachebuster": "^2.1.1",
    "exp-config": "^1.3.1",
    "express": "^4.13.4",
    "log": "^1.4.0",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "exp-deploy": "^1.3.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "mocha-cakes-2": "^1.1.1",
    "supertest": "^1.2.0"
  },
  "files": [
    "config",
    "lib",
    "test",
    "app.js",
    "cluster.js",
    ".eslintrc",
    ".eslintignore",
    ".nvmrc"
  ]
}
